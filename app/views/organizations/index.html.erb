<p id="notice"><%= notice %></p>

<h1>擁有的組織</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Descraption</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @organizations.each do |organization| %>
      <tr>
        <td><%= organization.title %></td>
        <td><%= organization.descraption %></td>
        <td><%= organization.user_id %></td>
        <td><%= link_to 'Show', organization %></td>
        <td><%= link_to 'Edit', edit_organization_path(organization) %></td>
        <td><%= link_to 'Destroy', organization, method: :delete, data: { confirm: '確定要刪除組織嗎？' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to '建立組織', new_organization_path %>
